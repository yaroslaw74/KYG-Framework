{
    "name": "yaroslaw74/kyg-framework",
    "description": "KYG Framework for Business",
    "type": "project",
    "license": [
        "GPL-3.0-or-later",
        "MIT"
    ],
    "version": "1.0.0",
    "authors": [
        {
            "name": "Kataev Yaroslav Georgievich",
            "email": "yaroslaw74@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "wiki": "https://github.com/yaroslaw74/KYG-Framework/wiki",
        "issues": "https://github.com/yaroslaw74/KYG-Framework/issues",
        "source": "https://github.com/yaroslaw74/KYG-Framework"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "^3.8",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.2",
        "fortawesome/font-awesome": "^6.5",
        "liborm85/composer-vendor-cleaner": "^1.7",
        "mnsami/composer-custom-directory-installer": "^2.0",
        "phpdocumentor/reflection-docblock": "^5.4",
        "phpstan/phpdoc-parser": "^1.29",
        "ramsey/uuid": "^4.7",
        "ramsey/uuid-doctrine": "^2.1",
        "squirrelphp/twig-php-syntax": "^1.8",
        "symfony/asset": "7.1.*",
        "symfony/console": "7.1.*",
        "symfony/doctrine-messenger": "7.1.*",
        "symfony/dotenv": "7.1.*",
        "symfony/expression-language": "7.1.*",
        "symfony/flex": "^2",
        "symfony/form": "7.1.*",
        "symfony/framework-bundle": "7.1.*",
        "symfony/http-client": "7.1.*",
        "symfony/intl": "7.1.*",
        "symfony/mailer": "7.1.*",
        "symfony/mime": "7.1.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "7.1.*",
        "symfony/process": "7.1.*",
        "symfony/property-access": "7.1.*",
        "symfony/property-info": "7.1.*",
        "symfony/requirements-checker": "^2.0",
        "symfony/runtime": "7.1.*",
        "symfony/security-bundle": "7.1.*",
        "symfony/serializer": "7.1.*",
        "symfony/string": "7.1.*",
        "symfony/translation": "7.1.*",
        "symfony/twig-bundle": "7.1.*",
        "symfony/validator": "7.1.*",
        "symfony/web-link": "7.1.*",
        "symfony/yaml": "7.1.*",
        "twbs/bootstrap": "^5.3",
        "twbs/bootstrap-icons": "^1.11",
        "twig/extra-bundle": "^3.10",
        "twig/twig": "^3.10"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "liborm85/composer-vendor-cleaner": true,
            "mnsami/composer-custom-directory-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Modules\\": "modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "requirements-checker": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.1.*"
        },
        "installer-paths": {
            "./public/{$name}": [
                "fortawesome/font-awesome",
                "twbs/bootstrap",
                "twbs/bootstrap-icons"
            ]
        },
        "dev-files": {
            "fortawesome/font-awesome": [
                ".github/",
                "css/all.css",
                "css/all.min.css",
                "css/brands.css",
                "css/brands.min.css",
                "css/fontawesome.css",
                "css/fontawesome.min.css",
                "css/regular.css",
                "css/regular.min.css",
                "css/solid.css",
                "css/solid.min.css",
                "css/v4-font-face.css",
                "css/v4-font-face.min.css",
                "css/v4-shims.css",
                "css/v4-shims.min.css",
                "css/v5-font-face.css",
                "css/v5-font-face.min.css",
                "js/brands.js",
                "js/brands.min.js",
                "js/conflict-detection.js",
                "js/conflict-detection.min.js",
                "js/fontawesome.js",
                "js/fontawesome.min.js",
                "js/regular.js",
                "js/regular.min.js",
                "js/solid.js",
                "js/solid.min.js",
                "js/v4-shims.js",
                "js/v4-shims.min.js",
                "js-packages/",
                "less/",
                "metadata/",
                "otfs/",
                "scss/",
                "sprites/",
                "svgs/",
                "webfonts/"
            ],
            "twbs/bootstrap": [
                ".github/",
                "build/",
                "dist/css/*.*.map",
                "dist/css/*.*.*.map",
                "dist/css/bootstrap-grid.css",
                "dist/css/bootstrap-grid.min.css",
                "dist/css/bootstrap-grid.rtl.css",
                "dist/css/bootstrap-grid.rtl.min.css",
                "dist/css/bootstrap-reboot.css",
                "dist/css/bootstrap-reboot.min.css",
                "dist/css/bootstrap-reboot.rtl.css",
                "dist/css/bootstrap-reboot.rtl.min.css",
                "dist/css/bootstrap-utilities.css",
                "dist/css/bootstrap-utilities.min.css",
                "dist/css/bootstrap-utilities.rtl.css",
                "dist/css/bootstrap-utilities.rtl.min.css",
                "dist/js/bootstrap.esm.js",
                "dist/js/bootstrap.esm.min.js",
                "dist/js/bootstrap.js",
                "dist/js/bootstrap.min.js",
                "dist/js/*.*.map",
                "dist/js/*.*.*.map",
                "dist/js/*.*.*.*.map",
                "js/dist/",
                "js/src/",
                "js/tests/",
                "js/index.esm.js",
                "js/index.umd.js",
                "nuget/",
                "scss/",
                "site/",
                ".babelrc.js",
                ".browserslistrc",
                ".bundlewatch.config.json",
                ".cspell.json",
                ".editorconfig",
                ".eslintignore",
                ".eslintrc.json",
                ".gitattributes",
                ".gitignore",
                ".npmrc",
                ".stylelintignore",
                ".stylelintrc.json",
                "hugo.yml",
                "package.js"
            ],
            "twbs/bootstrap-icons": [
                ".github/",
                "build/",
                "docs/",
                "font/*.scss",
                "icons/",
                ".browserslistrc",
                ".editorconfig",
                ".eslintignore",
                ".eslintrc.json",
                ".fantasticonrc.js",
                ".gitattributes",
                ".gitignore",
                ".stylelintrc.json",
                "hugo.yml",
                "svgo.config.mjs"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "symfony/browser-kit": "7.1.*",
        "symfony/css-selector": "7.1.*",
        "symfony/debug-bundle": "7.1.*",
        "symfony/maker-bundle": "^1.60",
        "symfony/phpunit-bridge": "^7.1",
        "symfony/stopwatch": "7.1.*",
        "symfony/web-profiler-bundle": "7.1.*",
        "twig/cssinliner-extra": "^3.10",
        "twig/inky-extra": "^3.10",
        "twig/markdown-extra": "3.10"
    }
}
