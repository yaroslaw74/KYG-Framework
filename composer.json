        "sebastian/code-unit-reverse-lookup": "^2.0",
        "sebastian/comparator": "^4.0",
        "sebastian/complexity": "^2.0",
        "sebastian/diff": "^4.0",
        "sebastian/environment": "^5.1",
        "sebastian/exporter": "^4.0",
