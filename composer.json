{
    "name": "yaroslaw74/kyg-framework",
    "description": "KYG Framework for Business",
    "type": "project",
    "license": [
        "GPL-3.0-or-later",
        "MIT"
    ],
    "authors": [
        {
            "name": "Kataev Yaroslav Georgievich",
            "email": "yaroslaw74@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "wiki": "https://github.com/yaroslaw74/KYG-Framework/wiki",
        "issues": "https://github.com/yaroslaw74/KYG-Framework/issues",
        "source": "https://github.com/yaroslaw74/KYG-Framework"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/doctrine-orm": "^4.0",
        "api-platform/graphql": "^4.0",
        "api-platform/symfony": "^4.0",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.3",
        "fortawesome/font-awesome": "^6.6",
        "friendsofsymfony/ckeditor-bundle": "^2.5",
        "liborm85/composer-vendor-cleaner": "^1.7",
        "liip/imagine-bundle": "^2.13",
        "monolog/monolog": "^3",
        "mnsami/composer-custom-directory-installer": "^2.0",
        "nelmio/cors-bundle": "^2.5",
        "phpdocumentor/reflection-docblock": "^5.4",
        "phpstan/phpdoc-parser": "^1.33",
        "shivas/versioning-bundle": "^4.1",
        "stof/doctrine-extensions-bundle": "^1.12",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "7.2.*",
        "symfony/asset-mapper": "7.2.*",
        "symfony/cache": "7.2.*",
        "symfony/clock": "7.2.*",
        "symfony/config": "7.2.*",
        "symfony/console": "7.2.*",
        "symfony/css-selector": "7.2.*",
        "symfony/dependency-injection": "7.2.*",
        "symfony/doctrine-bridge": "7.2.*",
        "symfony/doctrine-messenger": "7.2.*",
        "symfony/dotenv": "7.2.*",
        "symfony/emoji": "7.2.*",
        "symfony/error-handler": "7.2.*",
        "symfony/event-dispatcher": "7.2.*",
        "symfony/expression-language": "7.2.*",
        "symfony/filesystem": "7.2.*",
        "symfony/finder": "7.2.*",
        "symfony/flex": "^2",
        "symfony/form": "7.2.*",
        "symfony/framework-bundle": "7.2.*",
        "symfony/http-client": "7.2.*",
        "symfony/http-foundation": "7.2.*",
        "symfony/http-kernel": "7.2.*",
        "symfony/intl": "7.2.*",
        "symfony/mailer": "7.2.*",
        "symfony/messenger": "7.2.*",
        "symfony/mime": "7.2.*",
        "symfony/monolog-bridge": "7.2.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "7.2.*",
        "symfony/options-resolver": "7.2.*",
        "symfony/password-hasher": "7.2.*",
        "symfony/process": "7.2.*",
        "symfony/property-access": "7.2.*",
        "symfony/property-info": "7.2.*",
        "symfony/requirements-checker": "^2.0",
        "symfony/routing": "7.2.*",
        "symfony/runtime": "7.2.*",
        "symfony/scheduler": "7.2.*",
        "symfony/security-bundle": "7.2.*",
        "symfony/security-core": "7.2.*",
        "symfony/security-csrf": "7.2.*",
        "symfony/security-http": "7.2.*",
        "symfony/serializer": "7.2.*",
        "symfony/stimulus-bundle": "^2.21",
        "symfony/stopwatch": "7.2.*",
        "symfony/string": "7.2.*",
        "symfony/translation": "7.2.*",
        "symfony/twig-bridge": "7.2.*",
        "symfony/twig-bundle": "7.2.*",
        "symfony/type-info": "7.2.*",
        "symfony/uid": "7.2.*",
        "symfony/ux-chartjs": "^2.21",
        "symfony/ux-cropperjs": "^2.21",
        "symfony/ux-dropzone": "^2.21",
        "symfony/ux-icons": "^2.21",
        "symfony/ux-lazy-image": "^2.21",
        "symfony/ux-live-component": "^2.21",
        "symfony/ux-toggle-password": "^2.21",
        "symfony/ux-turbo": "^2.21",
        "symfony/ux-twig-component": "^2.21",
        "symfony/validator": "7.2.*",
        "symfony/var-dumper": "7.2.*",
        "symfony/var-exporter": "7.2.*",
        "symfony/web-link": "7.2.*",
        "symfony/workflow": "7.2.*",
        "symfony/yaml": "7.2.*",
        "twbs/bootstrap": "^5.3",
        "twbs/bootstrap-icons": "^1.11",
        "twig/cssinliner-extra": "^3.0|^4.0",
        "twig/extra-bundle": "^3.0|^4.0",
        "twig/inky-extra": "^3.0|^4.0",
        "twig/markdown-extra": "^3.0|^4.0",
        "twig/twig": "^3.0|^4.0",
        "vich/uploader-bundle": "^2.4"
    },
    "config": {
        "allow-plugins": {
            "liborm85/composer-vendor-cleaner": true,
            "mnsami/composer-custom-directory-installer": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true,
        "dev-files": {
            "match-case": false,
            "no-dev-only": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Modules\\": "modules/",
            "App\\Additions\\": "public/additions/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "requirements-checker": "script",
            "cache:clear": "symfony-cmd",
            "ckeditor:install --release=full --clear=drop": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd",
            "importmap:update": "symfony-cmd"
        },
        "post-install-cmd": [
            "php -f composer.php",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "php -f composer.php",
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*"
        },
        "installer-paths": {
            "./public/{$name}": [
                "fortawesome/font-awesome",
                "twbs/bootstrap",
                "twbs/bootstrap-icons"
            ]
        },
        "dev-files": {
            "*/*": [
                ".github/",
                "tests/",
                "docs/",
                ".gitattributes",
                ".gitignore"
            ],
            "fortawesome/font-awesome": [
                "js-packages/",
                "less/",
                "metadata/",
                "otfs/",
                "scss/",
                "sprites/",
                "svgs/",
                "webfonts/",
                "*/brands.*",
                "*/fontawesome.*",
                "*/regular.*",
                "*/solid.*",
                "*/v4-font-face.*",
                "*/v4-shims.*",
                "*/v5-font-face.*",
                "*/conflict-detection.*",
                "css/all.*",
                "css/svg-with-js.css",
                "js/all.js"
            ],
            "twbs/*": [
                "build/",
                ".*",
                "hugo.yml",
                "package.js",
                "package.json",
                "package-lock.json"
            ],
            "twbs/bootstrap": [
                "nuget/",
                "scss/",
                "site/",
                "*/*.map",
                "js/dist/",
                "js/src/",
                "js/index.*",
                "dist/css/bootstrap-grid.*",
                "dist/css/bootstrap-reboot.*",
                "dist/css/bootstrap-utilities.*",
                "dist/css/bootstrap.css",
                "dist/css/bootstrap.rtl.css",
                "dist/js/bootstrap.esm.*",
                "dist/js/bootstrap.js",
                "dist/js/bootstrap.min.js",
                "dist/js/bootstrap.bundle.js"
            ],
            "twbs/bootstrap-icons": [
                "icons/",
                "font/*.json",
                "font/*.scss",
                "font/bootstrap-icons.css",
                "bootstrap-icons.svg",
                "svgo.config.mjs",
                "svg-sprite.json"
            ]
        }
    },
    "require-dev": {
        "erusev/parsedown": "^1.7",
        "league/commonmark": "^2.5",
        "michelf/php-markdown": "^2.0",
        "phpunit/phpunit": "^9.6",
        "symfony/browser-kit": "7.2.*",
        "symfony/debug-bundle": "7.2.*",
        "symfony/dom-crawler": "7.2.*",
        "symfony/maker-bundle": "^1.61",
        "symfony/phpunit-bridge": "^7.1",
        "symfony/web-profiler-bundle": "7.2.*"
    }
}
