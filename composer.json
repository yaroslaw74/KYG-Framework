        "symfony/ux-cropperjs": "^2.22.1",
        "symfony/ux-dropzone": "^2.22.1",
        "symfony/ux-icons": "^2.22.1",
        "symfony/ux-toggle-password": "^2.22.1",
        "symfony/ux-turbo": "^2.22.1",
        "symfony/ux-twig-component": "^2.22.1",
        "symfony/validator": "7.2.*",
