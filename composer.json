                "*/v5-font-face.*",
                "*/conflict-detection.*",
                "css/all.*",
                "css/svg-with-js.css",
                "js/all.js"
            ],
            "twbs/bootstrap-icons": [
                "build/",
                ".*",
                "hugo.yml",
                "package.js",
                "package.json",
                "package-lock.json",
                "icons/",
                "font/*.json",
                "font/*.scss",
                "font/bootstrap-icons.css",
                "bootstrap-icons.svg",
                "svgo.config.mjs",
                "svg-sprite.json"
            ]
        }
    },
