{
    "name": "yaroslaw74/kyg-framework",
    "description": "KYG Framework for Business",
    "type": "project",
    "license": [
        "GPL-3.0-or-later",
        "MIT"
    ],
    "authors": [
        {
            "name": "Kataev Yaroslav Georgievich",
            "email": "yaroslaw74@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "wiki": "https://github.com/yaroslaw74/KYG-Framework/wiki",
        "issues": "https://github.com/yaroslaw74/KYG-Framework/issues",
        "source": "https://github.com/yaroslaw74/KYG-Framework"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "^3|^4",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": "^3",
        "doctrine/orm": "^3",
        "league/commonmark": "^2",
        "monolog/monolog": "^3",
        "phpdocumentor/reflection-docblock": "^5",
        "phpstan/phpdoc-parser": "^1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "7.1.*",
        "symfony/asset-mapper": "7.1.*",
        "symfony/cache": "7.1.*",
        "symfony/clock": "7.1.*",
        "symfony/config": "7.1.*",
        "symfony/console": "7.1.*",
        "symfony/css-selector": "7.1.*",
        "symfony/dependency-injection": "7.1.*",
        "symfony/doctrine-bridge": "7.1.*",
        "symfony/doctrine-messenger": "7.1.*",
        "symfony/dotenv": "7.1.*",
        "symfony/emoji": "7.1.*",
        "symfony/error-handler": "7.1.*",
        "symfony/event-dispatcher": "7.1.*",
        "symfony/expression-language": "7.1.*",
        "symfony/filesystem": "7.1.*",
        "symfony/finder": "7.1.*",
        "symfony/flex": "^2",
        "symfony/form": "7.1.*",
        "symfony/framework-bundle": "7.1.*",
        "symfony/http-client": "7.1.*",
        "symfony/http-foundation": "7.1.*",
        "symfony/http-kernel": "7.1.*",
        "symfony/intl": "7.1.*",
        "symfony/mailer": "7.1.*",
        "symfony/messenger": "7.1.*",
        "symfony/mime": "7.1.*",
        "symfony/monolog-bridge": "7.1.*",
        "symfony/monolog-bundle": "^3",
        "symfony/notifier": "7.1.*",
        "symfony/options-resolver": "7.1.*",
        "symfony/password-hasher": "7.1.*",
        "symfony/process": "7.1.*",
        "symfony/property-access": "7.1.*",
        "symfony/property-info": "7.1.*",
        "symfony/requirements-checker": "^2",
        "symfony/routing": "7.1.*",
        "symfony/runtime": "7.1.*",
        "symfony/scheduler": "7.1.*",
        "symfony/security-bundle": "7.1.*",
        "symfony/security-core": "7.1.*",
        "symfony/security-csrf": "7.1.*",
        "symfony/security-http": "7.1.*",
        "symfony/serializer": "7.1.*",
        "symfony/stimulus-bundle": "^2",
        "symfony/stopwatch": "7.1.*",
        "symfony/string": "7.1.*",
        "symfony/translation": "7.1.*",
        "symfony/twig-bridge": "7.1.*",
        "symfony/twig-bundle": "7.1.*",
        "symfony/type-info": "7.1.*",
        "symfony/uid": "7.1.*",
        "symfony/ux-icons": "^2",
        "symfony/ux-toggle-password": "^2",
        "symfony/ux-turbo": "^2",
        "symfony/validator": "7.1.*",
        "symfony/var-dumper": "7.1.*",
        "symfony/var-exporter": "7.1.*",
        "symfony/web-link": "7.1.*",
        "symfony/yaml": "7.1.*",
        "twig/cssinliner-extra": "^2.12|^3.0|^4.0",
        "twig/extra-bundle": "^2.12|^3.0|^4.0",
        "twig/inky-extra": "^2.12|^3.0|^4.0",
        "twig/markdown-extra": "^2.12|^3.0|^4.0",
        "twig/twig": "^2.12|^3.0|^4.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "sort-packages": true,
        "dev-files": {
            "match-case": false,
            "no-dev-only": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Modules\\": "modules/",
            "App\\Additions\\": "public/additions/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "requirements-checker": "script",
            "cache:clear": "symfony-cmd",
            "cache:warmup": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd",
            "importmap:update": "symfony-cmd",
            "asset-map:compile": "symfony-cmd"
        },
        "post-install-cmd": [
            "php -f composer.php",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "php -f composer.php",
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.1.*"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symfony/browser-kit": "7.1.*",
        "symfony/debug-bundle": "7.1.*",
        "symfony/dom-crawler": "7.1.*",
        "symfony/maker-bundle": "^1",
        "symfony/phpunit-bridge": "7.1.*",
        "symfony/web-profiler-bundle": "7.1.*"
    }
}
