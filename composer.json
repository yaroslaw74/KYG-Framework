{
    "name": "yaroslaw74/kyg-framework",
    "description": "KYG Framework for Business",
    "type": "project",
    "license": [
        "GPL-3.0-or-later",
        "MIT"
    ],
    "authors": [
        {
            "name": "Kataev Yaroslav Georgievich",
            "email": "yaroslaw74@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "wiki": "https://github.com/yaroslaw74/KYG-Framework/wiki",
        "issues": "https://github.com/yaroslaw74/KYG-Framework/issues",
        "source": "https://github.com/yaroslaw74/KYG-Framework"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "doctrine/dbal": "^4.1",
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.2",
        "fortawesome/font-awesome": "^6.6",
        "liborm85/composer-vendor-cleaner": "^1.7",
        "mnsami/composer-custom-directory-installer": "^2.0",
        "phpdocumentor/reflection-docblock": "^5.4",
        "phpstan/phpdoc-parser": "^1.30",
        "shivas/versioning-bundle": "^4.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "7.1.*",
        "symfony/asset-mapper": "7.1.*",
        "symfony/console": "7.1.*",
        "symfony/doctrine-messenger": "7.1.*",
        "symfony/dotenv": "7.1.*",
        "symfony/expression-language": "7.1.*",
        "symfony/flex": "^2.4",
        "symfony/form": "7.1.*",
        "symfony/framework-bundle": "7.1.*",
        "symfony/http-client": "7.1.*",
        "symfony/mailer": "7.1.*",
        "symfony/mime": "7.1.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "7.1.*",
        "symfony/process": "7.1.*",
        "symfony/property-access": "7.1.*",
        "symfony/property-info": "7.1.*",
        "symfony/runtime": "7.1.*",
        "symfony/security-bundle": "7.1.*",
        "symfony/serializer": "7.1.*",
        "symfony/stimulus-bundle": "^2.19",
        "symfony/string": "7.1.*",
        "symfony/translation": "7.1.*",
        "symfony/twig-bundle": "7.1.*",
        "symfony/ux-turbo": "^2.19",
        "symfony/validator": "7.1.*",
        "symfony/web-link": "7.1.*",
        "symfony/yaml": "7.1.*",
        "twbs/bootstrap": "^5.3",
        "twbs/bootstrap-icons": "^1.11",
        "twig/extra-bundle": "^3.13",
        "twig/twig": "^3.14"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "mnsami/composer-custom-directory-installer": true,
            "liborm85/composer-vendor-cleaner": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Modules\\": "modules/",
            "App\\Additions\\": "public/additions/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.1.*"
        },
        "installer-paths": {
            "./public/{$name}": [
                "fortawesome/font-awesome",
                "twbs/bootstrap",
                "twbs/bootstrap-icons"
            ]
        },
        "dev-files": {
            "*/*": [
                ".github/",
                "tests/",
                "docs/",
                ".gitattributes",
                ".gitignore"
            ],
            "fortawesome/font-awesome": [
                "js-packages/",
                "less/",
                "metadata/",
                "otfs/",
                "scss/",
                "sprites/",
                "svgs/",
                "webfonts/",
                "*/brands.*",
                "*/fontawesome.*",
                "*/regular.*",
                "*/solid.*",
                "*/v4-font-face.*",
                "*/v4-shims.*",
                "*/v5-font-face.*",
                "*/conflict-detection.*",
                "css/all.*",
                "css/svg-with-js.css",
                "js/all.js"
            ],
            "twbs/*": [
                "build/",
                ".*",
                "hugo.yml",
                "package.js",
                "package.json",
                "package-lock.json"
            ],
            "twbs/bootstrap": [
                "nuget/",
                "scss/",
                "site/",
                "*/*.map",
                "js/dist/",
                "js/src/",
                "js/index.*",
                "dist/css/bootstrap-grid.*",
                "dist/css/bootstrap-reboot.*",
                "dist/css/bootstrap-utilities.*",
                "dist/css/bootstrap.css",
                "dist/css/bootstrap.rtl.css",
                "dist/js/bootstrap.esm.*",
                "dist/js/bootstrap.js",
                "dist/js/bootstrap.min.js",
                "dist/js/bootstrap.bundle.js"
            ],
            "twbs/bootstrap-icons": [
                "icons/",
                "font/*.json",
                "font/*.scss",
                "font/bootstrap-icons.css",
                "bootstrap-icons.svg",
                "svgo.config.mjs",
                "svg-sprite.json"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "symfony/browser-kit": "7.1.*",
        "symfony/css-selector": "7.1.*",
        "symfony/debug-bundle": "7.1.*",
        "symfony/maker-bundle": "^1.61",
        "symfony/phpunit-bridge": "7.1.*",
        "symfony/web-profiler-bundle": "7.1.*"
    }
}
